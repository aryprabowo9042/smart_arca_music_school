{
  "functions": {
    "api/**/*.php": { "runtime": "vercel-php@0.7.1" }
  },
  "routes": [
    { "src": "/", "dest": "/api/admin/login.php" },
    { "src": "/login", "dest": "/api/admin/login.php" },
    { "src": "/logout", "dest": "/api/logout.php" },
    { "src": "/admin/(.*)", "dest": "/api/admin/$1" },
    { "src": "/guru/(.*)", "dest": "/api/guru/$1" },
    { "src": "/murid/(.*)", "dest": "/api/murid/$1" }
  ]
}
