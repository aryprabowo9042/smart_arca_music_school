{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.2"
    }
  },
  "routes": [
    { "src": "/api/admin/login", "dest": "/api/admin/login.php" },
    { "src": "/api/admin/honor", "dest": "/api/admin/honor.php" },
    { "src": "/api/murid/index", "dest": "/api/murid/index.php" },
    { "src": "/api/logout", "dest": "/api/logout.php" },
    { "src": "/api/cetak_kuitansi", "dest": "/api/cetak_kuitansi.php" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
