{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.2"
    }
  },
  "routes": [
    {
      "src": "/api/admin/(.*)",
      "dest": "/api/admin/$1"
    },
    {
      "src": "/api/guru/(.*)",
      "dest": "/api/guru/$1"
    },
    {
      "src": "/api/murid/(.*)",
      "dest": "/api/murid/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/$1"
    }
  ]
}
