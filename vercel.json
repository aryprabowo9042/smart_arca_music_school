{
  "functions": {
    "api/**/*.php": { "runtime": "vercel-php@0.7.1" }
  },
  "routes": [
    { "src": "/", "dest": "/api/admin/login.php" },
    { "src": "/admin", "dest": "/api/admin/index.php" },
    { "src": "/api/admin/(.*)", "dest": "/api/admin/$1" },
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ]
}
